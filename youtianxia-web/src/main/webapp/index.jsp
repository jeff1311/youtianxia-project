		if(winPos > winHeight / 2){
			$('.to-top').show();
		}else{
			$('.to-top').hide();
		}
	});
})
$('.to-top').click(function(){
	$("html,body").animate({scrollTop: $("#top").offset().top}, 300);
})
</script>
</html>